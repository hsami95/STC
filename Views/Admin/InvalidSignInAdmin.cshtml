@{
    ViewBag.Title = "InvalidSignInAdmin";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model STC_online.Models.SignUp
<div class="row">
    <img src="~/contents/image/hostelSignin.jpg" class="img-responsive col-md-6">
    <div class="modal-content pull-right col-md-5">

        <font color="415c81" style="font-family: cursive" size="6"> Admin Sign In</font>

        <div class="modal-body">
            <form class="form col-md-12 " method="post" action="@Url.Action("AdminPanal","Admin")">
                <div class="form-group">
                    @*<input type="text" class="form-control input-lg" placeholder="CNIC">*@
                    <div class="alert-danger"><h3>Please Enter Valid Email & Password</h3></div>
                    @Html.TextBoxFor(x => x.cnic, htmlAttributes: new
                {
                    @class = "form-control input-lg",
                    @placeholder = "CNIC"
                })
                </div>
                <div class="form-group">
                    @Html.TextBoxFor(x => x.password, htmlAttributes: new
                    {
                        @class = "form-control input-lg",
                        @placeholder = "Password",
                        @type = "password"

                    })
                </div>

                <div class="form-group">

                    <button class="btn btn-primary btn-lg btn-block">Sign In</button>

                </div>
            </form>
        </div>
        <div class="modal-footer">
            <div class="col-md-12">
                <a href="~/Home"> <button class="btn" data-dismiss="modal" aria-busy="True">Cancel</button></a>
                <br /><br /><br />
            </div>
        </div>
    </div>

</div>
<br /><br /><br /><br /><br /><br /><br /><br />

